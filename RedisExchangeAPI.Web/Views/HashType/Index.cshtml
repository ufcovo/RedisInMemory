@model Dictionary<string, string>
@{
    ViewData["Title"] = "Index";
}


<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <input type="text" name="val" />
    <button type="submit">Save</button>
</form>

@foreach (var item in Model)
{

    <h2>@item.Key  -> @item.Value <a asp-action="DeleteItem" asp-route-name="@item.Key">Delete</a></h2>
}